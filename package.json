{
  "name": "split-it",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint",
    "start": "node -r source-map-support/register ./dist/index.js",
    "tsc_start": "npx tsc && npm start",
    "test": "npm run dbDropTest; npm run dbCreateTest; npm run dbMigrateTest; NODE_ENV=test npm run test_mocha; npm run dbDropTest",
    "test_mocha": "mocha -r ts-node/register ./test/*.spec.ts ./test/**/*.spec.ts",
    "dbCreate": "psql -c 'CREATE DATABASE split_it'",
    "dbCreateTest": "psql -c 'CREATE DATABASE split_it_test'",
    "dbDropTest": "psql -c 'DROP DATABASE split_it_test'",
    "dbMigrate": "npx db-migrate up -e production",
    "dbMigrateTest": "npx db-migrate up -e test"
  },
  "dependencies": {
    "@types/clear": "^0.1.0",
    "clear": "^0.1.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "inquirer": "^7.1.0",
    "pg": "^8.0.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
